{
  "name": "leetcode-algorithms-practice",
  "version": "1.0.0",
  "description": "My LeetCode Algorithms Practice",
  "main": "index.js",
  "scripts": {
    "lint": "eslint '**/*.js'",
    "lint:fix": "eslint '**/*.js' --fix",
    "lint:fixstaged": "eslint --fix",
    "postlint": "echo lint valid",
    "init": "npm i",
